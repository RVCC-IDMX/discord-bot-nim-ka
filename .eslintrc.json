{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "import/no-dynamic-require": "off",
        "import/no-extraneous-dependencies": "off",
        "global-require": "off",
        "no-restricted-syntax": "off"
    }
}